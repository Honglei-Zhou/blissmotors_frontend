    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "devDeploy": "aws s3 sync --acl public-read --delete dist/ s3://blissmotors-website-frontend-dev && npm run devInvalidate",
    "deploy": "aws s3 sync --acl public-read --delete dist/ s3://blissmotors-website-frontend && npm run invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EU6EQN4LVY890 --paths '/*'",
    "devInvalidate": "aws cloudfront create-invalidation --distribution-id E33J04TYOHRLG2 --paths '/*'"
  },
